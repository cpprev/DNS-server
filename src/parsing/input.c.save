server_config *parse_server_config(const char *path)
{
    string *in = read_file(path);
    server_config *cfg = NULL;
    string *dummy = string_init();
    for (size_t i = 0; i < in->size; ++i)
    {
        char c = in->arr[i];
        if (c == '\n' or i == in->size - 1)
        {
